{
  "family": "my-task-family",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "c_client",
      "image": "PLACEHOLDER_CLIENT_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    },
    {
      "name": "c_server",
      "image": "PLACEHOLDER_SERVER_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
